{
  "format": "custom",
  "id": "posts",
  "label": "Posts",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "uid": {
        "type": "UID",
        "config": {
          "label": "UID"
        }
      },
      "image": {
        "type": "Image",
        "config": {
          "label": "Image",
          "constraint": {},
          "thumbnails": []
        }
      },
      "title": {
        "type": "Text",
        "config": {
          "label": "Title",
          "placeholder": "Título do post"
        }
      },
      "content": {
        "type": "StructuredText",
        "config": {
          "label": "Content",
          "placeholder": "Texto descritivo do post",
          "allowTargetBlank": true,
          "multi": "paragraph,preformatted,heading1,heading2,heading3,heading4,heading5,heading6,strong,em,hyperlink,image,embed,list-item,o-list-item,rtl"
        }
      },
      "author": {
        "type": "Text",
        "config": {
          "label": "Author",
          "placeholder": "Autor do post"
        }
      },
      "date": {
        "type": "Date",
        "config": {
          "label": "Date",
          "placeholder": "Data de criação do post"
        }
      },
      "slices": {
        "type": "Slices",
        "fieldset": "Slice Zone"
      }
    }
  }
}
